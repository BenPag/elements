{
  "name": "@inovex/elements",
  "version": "0.1.1",
  "description": "This is the home of the stencil UI components.",
  "repository": "ssh://git@gitlab.inovex.de:2424/inovex-elements/core.git",
  "author": "Patrick Hillert <phillert@inovex.de>",
  "license": "UNLICENSED",
  "main": "dist/inovex-elements.js",
  "module": "dist/inovex-elements.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "webpack-plugin.js"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.storybook": "yarn run build && build-storybook",
    "dev": "sd concurrent \"yarn build --watch\" \"yarn run storybook\" ",
    "serve": "yarn run dev",
    "start": "yarn run dev",
    "lint": "tslint --project tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "test": "jest --no-cache --coverage",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@material/button": "^0.37.0",
    "@material/card": "^0.37.0",
    "@material/checkbox": "^0.37.0",
    "@material/chips": "^0.37.0",
    "@material/form-field": "^0.37.0",
    "@material/radio": "^0.37.0",
    "@material/select": "^0.37.0",
    "@material/textfield": "^0.37.0",
    "@stencil/core": "^0.10.9",
    "classnames": "^2.2.6",
    "flatpickr": "^4.4.6",
    "tooltip.js": "^1.2.0"
  },
  "devDependencies": {
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-knobs": "^3.4.1",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addon-options": "^3.4.1",
    "@storybook/addons": "^3.4.1",
    "@storybook/vue": "^3.4.1",
    "@types/jest": "^21.1.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-vue": "^2.0.2",
    "babel-register": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "jest": "^21.2.1",
    "marked": "^0.3.19",
    "moment": "^2.22.2",
    "sass-loader": "^7.0.3",
    "storybook-readme": "^3.3.0",
    "tslint": "^5.10.0",
    "tslint-ionic-rules": "0.0.14",
    "vue": "^2.5.2",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
