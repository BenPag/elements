{
  "name": "@inovex/elements",
  "version": "0.4.0",
  "description": "This is the home of the stencil UI components.",
  "repository": "ssh://git@gitlab.inovex.de:2424/inovex-elements/core.git",
  "author": "Patrick Hillert <phillert@inovex.de>",
  "license": "UNLICENSED",
  "main": "dist/inovex-elements.js",
  "module": "dist/inovex-elements.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "webpack-plugin.js"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "postbuild": "node ./scripts/postbuild.js",
    "build.storybook": "yarn run build && build-storybook -s ./.storybook/static",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev": "sd concurrent \"yarn build --watch\" \"yarn run storybook\" ",
    "serve": "yarn run dev",
    "start": "yarn run dev",
    "lint": "tslint --project tsconfig.json",
    "lint.fix": "tslint --project tsconfig.json --fix",
    "storybook": "start-storybook -p 6006 -s ./.storybook/static",
    "test": "jest --no-cache --coverage",
    "test.watch": "jest --watch --no-cache",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "@material/button": "^0.37.0",
    "@material/card": "^0.37.0",
    "@material/checkbox": "^0.37.0",
    "@material/chips": "^0.39.3",
    "@material/fab": "^0.39.3",
    "@material/form-field": "^0.37.0",
    "@material/icon-button": "0.39.0-0",
    "@material/list": "^0.39.1",
    "@material/menu": "^0.39.1",
    "@material/radio": "^0.37.0",
    "@material/ripple": "^0.38.1",
    "@material/select": "^0.39.0",
    "@material/snackbar": "^0.39.1",
    "@material/tab": "^0.39.1",
    "@material/tab-bar": "^0.40.0",
    "@material/tab-indicator": "^0.39.1",
    "@material/tab-scroller": "^0.39.1",
    "@material/textfield": "^0.38.1",
    "@material/typography": "^0.39.0",
    "@stencil/core": "0.12.4",
    "classnames": "^2.2.6",
    "flatpickr": "^4.4.6",
    "tooltip.js": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@stencil/sass": "^0.1.0",
    "@stencil/utils": "latest",
    "@storybook/addon-actions": "4.0.0-alpha.22",
    "@storybook/addon-knobs": "4.0.0-alpha.22",
    "@storybook/addon-options": "4.0.0-alpha.22",
    "@storybook/addon-storysource": "4.0.0-alpha.22",
    "@storybook/addon-viewport": "4.0.0-alpha.22",
    "@storybook/addons": "4.0.0-alpha.22",
    "@storybook/core": "4.0.0-alpha.22",
    "@storybook/html": "4.0.0-alpha.22",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^21.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.2",
    "babel-runtime": "^6.26.0",
    "camelcase": "^5.0.0",
    "conventional-changelog-cli": "^2.0.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "fs-extra": "^7.0.0",
    "jest": "^21.2.1",
    "marked": "^0.5.0",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "sass-loader": "^7.0.3",
    "tslint": "^5.10.0",
    "tslint-ionic-rules": "0.0.19",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
