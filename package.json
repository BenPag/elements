{
  "name": "stencil-components",
  "version": "0.0.1",
  "description": "This is the home of the stencil UI components.",
  "repository": "ssh://git@gitlab.inovex.de:2424/inovex-elements/stencil-components.git",
  "author": "Patrick Hillert <phillert@inovex.de>",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "dev": "sd concurrent \"yarn build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "lint": "tslint --project tsconfig.json",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "^0.9.7",
    "flatpickr": "^4.4.6",
    "tooltip.js": "^1.2.0"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "tslint": "^5.10.0",
    "tslint-ionic-rules": "0.0.14"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
