  "scripts": {
    "build": "stencil build --docs && node scripts/generateWrappedIcons.js",
    "postbuild": "./scripts/postbuild.sh",
    "build.james": "yalc publish --no-sig && yarn build && yalc push --changed",
    "build.storybook": "yarn run build && build-storybook -s ./.storybook/static",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "start": "sd concurrent \"stencil build --docs --watch\" \"yarn run storybook\" ",
    "lint": "tslint --project tsconfig.json",
    "lint.fix": "tslint --project tsconfig.json --fix",
    "storybook": "start-storybook -p 6006 -s ./.storybook/static",
    "test": "stencil test --e2e --ci",
    "test.watch": "stencil test --e2e --spec --watch",
    "version": "yarn changelog && sed -i'' -e 's/gitlab.inovex.de:2424/gitlab.inovex.de/g' ./CHANGELOG.md && rm ./CHANGELOG.md-e && git add CHANGELOG.md"
  },
