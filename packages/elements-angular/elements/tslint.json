{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "class-name": false,
    "only-arrow-functions": false,
    "space-before-function-paren": false
  }
}
