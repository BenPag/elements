{
  "name": "workspace",
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "targets": {
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "syncVersions": true,
        "skipProjectChangelog": true,
        "skipCommit": true,
        "dryRun": false,
        "push": false,
        "postTargets": ["release"]
      },
      "configurations": {
        "prerelease": {
          "releaseAs": "prerelease",
          "skipRootChangelog": true,
          "postTargets": ["release"]
        }
      }
    },
    "release": {
      "command": "node ./scripts/release.js"
    },
    "deploy-github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "{tag}",
        "notes": "{notes}"
      }
    }
  },
  "implicitDependencies": [
    "elements",
    "elements-angular",
    "elements-react",
    "elements-vue"
  ],
  "tags": []
}
